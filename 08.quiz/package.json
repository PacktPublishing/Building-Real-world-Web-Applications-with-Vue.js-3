    "dev:server": "cd server && npm run dev",
    "dev:open": "node ./open.mjs"
  },
  "author": "Joran Quinten <joran@joranquinten.nl>",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.7.0",
    "chalk": "^5.3.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "open": "^9.1.0",
    "ts-node": "^10.9.1"
  }
}
